import { Console } from System;

void Main() {
    const result = fibonacci(5);
    Console.WriteLine("The result of fibonacci(5) is " + result.ToString());
}

int fibonacci(int i) {
    unless i > 0 {
        return -1;
    }

    if i == 1 || i == 2 {
        return 1;
    }

    return fibonacci(i - 1) + fibonacci(i - 2);
}
