{
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.enableEditorConfigSupport": true,
  "omnisharp.useGlobalMono": "never",
  "omnisharp.defaultLaunchSolution": "${workspaceFolder}/src/Todl.sln",
  "dotnet-test-explorer.testProjectPath": "**/*Test*.csproj",
  "files.associations": {
    "*.tdlproj": "xml"
  },
  "azureFunctions.projectSubpath": "src/Todl.Playground",
  "azureFunctions.deploySubpath": "out/Release/src/Todl.Playground/publish",
  "azureFunctions.projectLanguage": "C#",
  "azureFunctions.projectRuntime": "~4",
  "debug.internalConsoleOptions": "neverOpen",
  "azureFunctions.preDeployTask": "publish (functions)"
}